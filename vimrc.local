" Local settings
nnoremap <leader>eco :e config/config.json<cr>
nnoremap <leader>ede :e config/development.json<cr>
nnoremap <leader>est :e config/staging.json<cr>
nnoremap <leader>eap :e config/appConfig.json<cr>
nnoremap <leader>ewp :e webpack.config.js<cr>

" Truecolor support
if !&diff
  let &t_8f="\e[38;2;%ld;%ld;%ldm"
  let &t_8b="\e[48;2;%ld;%ld;%ldm"
  set termguicolors
endif

" Gist Notes
noremap <leader>it :Gist 2a7db40268fa3a65faa9<cr>
